{
	"name": "Todo List",
	"version": "1",
	"manifest_version": 2,
	"description": "Extencion para generar una lista  de tareas, sincronizada con firebase",
	"icons": {
		"32": "img/icons/equalizer_32.png",
		"48": "img/icons/equalizer_48.png",
		"128": "img/icons/equalizer_128.png"
	},
	"background": {"scripts": ["background.js"]},
    "developer": { "name": "Marcelo joc" },
    "minimum_opera_version": "30",
	"options_page": "options/options.html",

    "permissions": [
        "storage",
        "bookmarks",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]

}